
@-webkit-keyframes slidein
	from
		opacity 0
		margin-left 100% 
	to
		opacity 1
		margin-left 0%

@-moz-keyframes slidein
	from
		opacity 0
		margin-left 100% 
	to
		opacity 1
		margin-left 0%



@-webkit-keyframes popup
	from
		-webkit-transform none
	to
		-webkit-transform none
	50%
		-webkit-transform scale(1.1)

@-moz-keyframes popup
	from
		-moz-transform none
	to
		-moz-transform none
	50%
		-moz-transform scale(1.1)